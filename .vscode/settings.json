{
  "go.inferGopath": false,
  "sqltools.connections": [{
    "database": "/Users/ms/tmp/movielight.db",
    "driver": "SQLite",
    "name": "movielight",
    "previewLimit": 50
  }],
  "go.testFlags": [
    "-tags",
    "fts5"
  ],
  "go-tests-outline.go-test-params": "-tags fts5",
  "vetur.format.defaultFormatterOptions": {
    "prettier": {
      "singleQuote": true
    }
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "vue"
  ],


  "eslint.autoFixOnSave": true,
  "editor.formatOnSave": false,
  "vetur.validation.template": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "jshint.options": {
    "esversion": 10,
    "asi": true
  },
  "go.diagnostic.vulncheck": "Imports",

}